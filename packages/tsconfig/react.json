    "compilerOptions": {
      "jsx": "react-jsx",
      "lib": [
        "ES2017",
        "DOM"
      ],
      "esModuleInterop": true,
      "module": "ESNext",
      "declaration": true,
      "outDir": "./dist",
      "target": "es6"
    }
  }